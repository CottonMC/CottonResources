{
  "schemaVersion": 1,
  "id": "cotton-resources",
  "version": "${version}",
  "name": "Cotton Resources",
  "icon": "assets/c/icon.png",
  "description": "Reference mod for ores, ingots, tools, liquids, and all you can imagine",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.cottonmc.resources.CottonResources"
    ],
    "rei_plugins": [
      "io.github.cottonmc.resources.compat.REICompat"
    ]
  },
  "mixins": [
    "cotton-resources.common.json"
  ],
  "authors": [
    "the Cotton Team"
  ],
  "contact": {
    "homepage": "https://github.com/CottonMC/CottonResources",
    "sources": "https://github.com/CottonMC/CottonResources",
    "issues": "https://github.com/CottonMC/CottonResources/issues"
  },

  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*",
    "cotton": "*"
  }
}
